{
  "defaultLanguage": "exinc",
  "editor": "vim",
  "languages": {
    "cpp": {
      "template": "template.cpp",
      "file": "%{problem-code}.cpp",
      "preprocess": [
        "g++ \"%{file}\" -std=c++17"
      ],
      "exec": "./a.out",
      "artifacts": {
        "a.out": {
          "executable": true
        }
      },
      "submitor": "c++17"
    },
    "exinc": {
      "template": "template.cpp",
      "file": "%{problem-code}.cpp",
      "submitFile": "%{problem-code}.pre.cpp",
      "preprocess": [
        "exinc -i \"%{file}\" -o \"%{submit-file}\" -c"
      ],
      "exec": "./a.out",
      "artifacts": {
        "a.out": {
          "executable": true
        }
      },
      "submitor": "c++17"
    }
  },
  "submitor": {
    "c++17": {
      "codeforces": {
        "typeid": 54
      }
    }
  },
  "credentials": {
    "codeforces": {
      "handle": "",
      "password": ""
    }
  }
}